<?xml version="1.0" encoding="UTF-8"?>
<plugin_manifest xmlns="http://www.elgg.org/plugin_manifest/1.8">
  <name>Izap seo</name>
  <author>Izap Team</author>
  <version>1</version>
  <category>izap</category>
  <blurb>For elgg SEO</blurb>
  <description>For elgg SEO.</description>
  <website>http://pluginlotto.org/</website>
  <copyright>See COPYRIGHT.txt</copyright>
  <license>GNU General Public License version 2</license>
  
  <requires>
    <type>elgg_version</type>
    <version>1.8</version>
  </requires>
 
   <requires>
      <type>plugin</type>
      <name>izap-elgg-bridge</name>
      <version>2.1.3</version>
    </requires>
    
    <requires>
      <type>php_extension</type>
      <name>curl</name>
    </requires>
    
    <suggests>
      <type>php_extension</type>
      <name>pdo_sqlite</name>
    </suggests>
    
    <priority>
      <plugin>izap-elgg-bridge</plugin>
      <priority>after</priority>
    </priority>
    
  <screenshot>
    <description>Izap seo admin settings</description>
    <path>screenshots/izap-seo-admin-section.png</path>
  </screenshot>
  <screenshot>
    <description>Izap video with seo</description>
    <path>screenshots/izap-video-with-seo.png</path>
  </screenshot>
  <screenshot>
    <description>New blog</description>
    <path>screenshots/new-blog.png</path>
  </screenshot>
    
  <activate_on_install>false</activate_on_install>
  <admin_interface>advanced</admin_interface>
</plugin_manifest>
